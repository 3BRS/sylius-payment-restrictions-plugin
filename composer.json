{
    "name": "3brs/sylius-payment-restrictions-plugin",
    "type": "sylius-plugin",
    "description": "Payment restriction plugin for Sylius",
    "keywords": [
        "sylius",
        "sylius-plugin",
        "plugin",
        "payment",
        "restriction"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "sylius/sylius": "^2.0"
    },
    "require-dev": {
        "behat/behat": "^3.22",
        "behat/mink-selenium2-driver": "^1.7",
        "consolidation/robo": "^4.0 || ^5.0",
        "dbrekelmans/bdi": "^1.3",
        "dmore/behat-chrome-extension": "^1.4",
        "dmore/chrome-mink-driver": "^2.9",
        "doctrine/cache": "^2.2",
        "doctrine/data-fixtures": "^1.7",
        "friends-of-behat/mink": "^1.11",
        "friends-of-behat/mink-browserkit-driver": "^1.6",
        "friends-of-behat/mink-debug-extension": "^2.1",
        "friends-of-behat/mink-extension": "^2.7",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/symfony-extension": "^2.6.2",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/variadic-extension": "^1.6",
        "hwi/oauth-bundle": "^2.2",
        "jangregor/phpstan-prophecy": "^2.0",
        "lchrusciel/api-test-case": "^5.3",
        "matthiasnoback/symfony-config-test": "^5.1",
        "matthiasnoback/symfony-dependency-injection-test": "^5.1",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.6",
        "nyholm/psr7": "^1.8",
        "phparkitect/phparkitect": "^0.6",
        "phpspec/prophecy-phpunit": "^2.2",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1.13",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^10.5",
        "psr/event-dispatcher": "^1.0",
        "rector/rector": "^2.0",
        "robertfausk/behat-panther-extension": "^1.1",
        "sylius-labs/coding-standard": "^4.4",
        "sylius-labs/suite-tags-extension": "~0.2",
        "symfony/browser-kit": "^6.4 || ^7.2",
        "symfony/debug-bundle": "^6.4 || ^7.2",
        "symfony/dotenv": "^6.4 || ^7.2",
        "symfony/flex": "^2.4",
        "symfony/web-profiler-bundle": "^6.4 || ^7.2",
        "symplify/monorepo-builder": "^11.0"
    },
    "conflict": {
        "api-platform/symfony": "<v4.1.0",
        "api-platform/core": "<2.7.13 || >=3.0",
        "babdev/pagerfanta-bundle": "<3.6.1",
        "doctrine/collections": "<1.7.0",
        "doctrine/data-fixtures": "<1.5.1",
        "doctrine/dbal": "<2.13.3",
        "doctrine/doctrine-bundle": "<2.13.1",
        "friendsofsymfony/rest-bundle": "<3.1.0",
        "jms/serializer-bundle": "<4.2.0",
        "masterminds/html5": "<2.7.5",
        "knplabs/knp-menu": "<3.3.0",
        "lexik/jwt-authentication-bundle": "<2.12",
        "payum/core": "<1.7.3",
        "polishsymfonycommunity/symfony-mocker-container": "<1.0.6",
        "sylius/resource-bundle": "<1.11.0",
        "sylius/sylius": "<2.0.6",
        "symfony/css-selector": "<4.4.24",
        "symfony/framework-bundle": "<6.4.22",
        "symfony/mime": "<5.4.0",
        "symfony/validator": "7.3.0",
        "symfony/web-link": "<5.3.0",
        "symplify/easy-coding-standard": "<12.5.20",
        "twig/twig": "<2.14.7",
        "webmozart/assert": "<1.11.0",
        "willdurand/negotiation": "<3.0",
        "composer/pcre": "<3.3"

    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ThreeBRS\\SyliusPaymentRestrictionPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\ThreeBRS\\SyliusPaymentRestrictionPlugin\\": "tests/Application/src/",
            "Tests\\ThreeBRS\\SyliusPaymentRestrictionPlugin\\Behat\\": "tests/Behat/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "symfony/flex": false,
            "php-http/discovery": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
